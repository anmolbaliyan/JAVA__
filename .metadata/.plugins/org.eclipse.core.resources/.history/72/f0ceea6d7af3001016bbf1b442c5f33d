package in.co.rays;

public class SwapNumber2 {
	public static void main(String[] args) {
		int a = 5;
		inr b = 10;
		
		System.out.println("a =" + a);
		System.out.println("a =" + a);
		
		System.out.println("------------");
		
		a = a+b;
		b = a-b;
		a = a-b;
		
		System.out.println("a =" + a);
		System.out.println("b =" + b);
	}

}
